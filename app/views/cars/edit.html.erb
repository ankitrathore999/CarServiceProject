<div class="container" align="center">
  <%= form_for @car, url: {action: :update} do |f| %>   
    <div>   
      <label>car Name</label>   
      <%= f.text_field :name %>   
    </div><br>
    <div>   
      <label>Number</label>   
      <%= f.text_field :number %>   
    </div><br>  
     <div>   
      <label>Model</label>   
      <%= f.text_field :model %>   
    </div><br>  
    <div>
      <label>Company</label>   
      <%= f.text_field :company %>   
    </div><br>        
    <div>   
      <%= link_to 'Back', { controller: 'cars', action: 'index'} %>   
      <%= f.submit 'Update car' %>   
    </div>   
  <% end %>   
</div>  
